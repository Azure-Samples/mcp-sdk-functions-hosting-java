{
    "version": "2.0",
    "extensions": {
        "http": {
            "routePrefix": ""
        }
    },
    "customHandler": {
        "description": {
            "defaultExecutablePath": "java",
            "workingDirectory": "",
            "arguments": ["-Dquarkus.http.port=%FUNCTIONS_CUSTOMHANDLER_PORT%",
                          "-jar",
                          "quarkus-mcp-1.0.0-runner.jar"]
        },
        "enableForwardingHttpRequest": true,
        "enableHttpProxyingRequest": true
    }
}