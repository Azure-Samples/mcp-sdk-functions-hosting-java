{
    "version": "2.0",
    "configurationProfile": "mcp-custom-handler",
    "customHandler": {
        "description": {
            "defaultExecutablePath": "java",
            "arguments": ["-agentlib:jdwp=transport=dt_socket,server=y,suspend=n,address=5005",
                          "-jar", "@runner.jar.name@"]
        }, 
        "http": {
            "DefaultAuthorizationLevel": "anonymous"
        },
        "port": "8080"
    }
}